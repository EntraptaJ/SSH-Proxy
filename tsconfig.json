{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",

    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "importHelpers": false,
    "useDefineForClassFields": false
  }
}
